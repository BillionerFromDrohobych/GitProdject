<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="misterpanchak.com.ostapuchi">

    <application
        android:allowBackup="true"
        android:icon="@drawable/start"
        android:label="SightMap"
        android:roundIcon="@drawable/start"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"  android:theme="@style/AppTheme1">>

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".ActivityActivity" />
        <activity android:name=".SightActivity"></activity>
    </application>

</manifest>